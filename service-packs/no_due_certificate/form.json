{
  "formId": "FORM_NO_DUE_CERTIFICATE",
  "version": "1.0.0",
  "pages": [
    {
      "pageId": "PAGE_APPLICATION",
      "title": "Application Details",
      "sections": [
        {
          "sectionId": "SEC_AUTHORITY",
          "title": "Authority",
          "fields": [
            {
              "key": "authority_id",
              "label": "Select Authority",
              "type": "enum",
              "required": true,
              "ui": {
                "widget": "select",
                "options": [
                  {
                    "value": "PUDA",
                    "label": "PUDA"
                  },
                  {
                    "value": "GMADA",
                    "label": "GMADA"
                  },
                  {
                    "value": "GLADA",
                    "label": "GLADA"
                  },
                  {
                    "value": "BDA",
                    "label": "BDA"
                  }
                ],
                "fillFromProperty": "authority_id"
              }
            }
          ]
        },
        {
          "sectionId": "SEC_APPLICANT",
          "title": "Applicant Details",
          "fields": [
            {
              "sharedSection": "applicant"
            }
          ]
        }
      ]
    },
    {
      "pageId": "PAGE_PROPERTY",
      "title": "Property Details",
      "sections": [
        {
          "sectionId": "SEC_PROPERTY",
          "title": "Property Information",
          "fields": [
            {
              "key": "property.upn",
              "label": "Unique Property Number (UPN / Bhu-Aadhar)",
              "type": "string",
              "required": true,
              "ui": {
                "widget": "upn-picker"
              }
            },
            {
              "key": "property.area_sqyd",
              "label": "Area (Sq. Yards)",
              "type": "number",
              "required": false,
              "ui": {
                "fillFromProperty": "area_sqyd"
              }
            },
            {
              "key": "property.authority_name",
              "label": "Authority Name",
              "type": "string",
              "required": false,
              "ui": {
                "fillFromProperty": "authority_id"
              }
            },
            {
              "key": "property.plot_no",
              "label": "Plot Number",
              "type": "string",
              "required": false,
              "ui": {
                "fillFromProperty": "property_number"
              }
            },
            {
              "key": "property.type",
              "label": "Property Type",
              "type": "enum",
              "required": false,
              "ui": {
                "fillFromProperty": "usage_type",
                "options": [
                  {
                    "value": "RESIDENTIAL",
                    "label": "Residential"
                  },
                  {
                    "value": "COMMERCIAL",
                    "label": "Commercial"
                  },
                  {
                    "value": "INDUSTRIAL",
                    "label": "Industrial"
                  }
                ]
              }
            },
            {
              "key": "property.scheme_name",
              "label": "Scheme Name",
              "type": "string",
              "required": false,
              "ui": {
                "fillFromProperty": "scheme_name"
              }
            }
          ]
        }
      ]
    },
    {
      "pageId": "PAGE_PAYMENT",
      "title": "Payment Status",
      "sections": [
        {
          "sectionId": "SEC_PAYMENT",
          "title": "Payment Schedule & Status",
          "fields": []
        }
      ]
    }
  ]
}
